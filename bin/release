#!/usr/bin/env bash
#bin/release <build-dir>

cat << EOF
---
config_vars:
  PATH: /app/luajit/bin:/usr/local/bin:/usr/bin:/bin
  LD_LIBRARY_PATH: /app/luajit/lib
  LUA_CPATH: ./?.so;/app/luajit/lib/lua/5.1/?.so
  LUA_PATH: ./?.lua;/app/luajit/share/lua/5.1/?.lua;/app/packages/share/lua/5.1/?/init.lua
  STACK: $STACK
EOF